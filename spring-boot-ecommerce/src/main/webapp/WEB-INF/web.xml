<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>EcommerceApplication</servlet-name>
  	<display-name>EcommerceApplication</display-name>
  	<description></description>
  	<servlet-class>com.javainuse.EcommerceApplication</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UserController</servlet-name>
  	<display-name>UserController</display-name>
  	<description></description>
  	<servlet-class>com.javainuse.controller.UserController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UserRepository</servlet-name>
  	<display-name>UserRepository</display-name>
  	<description></description>
  	<servlet-class>com.javainuse.db.UserRepository</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>User</servlet-name>
  	<display-name>User</display-name>
  	<description></description>
  	<servlet-class>com.javainuse.model.User</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>EcommerceApplication</servlet-name>
  	<url-pattern>/EcommerceApplication</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UserController</servlet-name>
  	<url-pattern>/UserController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UserRepository</servlet-name>
  	<url-pattern>/UserRepository</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>User</servlet-name>
  	<url-pattern>/User</url-pattern>
  </servlet-mapping>
</web-app>
